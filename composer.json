{
    "name": "bwannduke/behat-twig-report-extension",
    "description": "Create Behat 3 test reports with Twig templates",
    "type": "behat-extension",
    "authors": [{
            "name": "Christian Hartinger",
            "email": "c.k.hartinger@gmail.com"
        },
        {
            "name": "Ben Wann",
            "email": "bwann@dukemfg.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.3.2",
        "behat/behat": "3.*.*",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/process": "*"
    },
    "autoload": {
        "psr-0": {
            "chartinger\\Behat\\TwigReportExtension": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
